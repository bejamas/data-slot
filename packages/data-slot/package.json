{
  "name": "data-slot",
  "version": "0.2.4",
  "type": "module",
  "sideEffects": false,
  "description": "Headless UI components for vanilla JavaScript. Convenience re-exports from @data-slot/* packages.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/core.d.ts",
        "default": "./dist/core.js"
      },
      "require": {
        "types": "./dist/core.d.cts",
        "default": "./dist/core.cjs"
      }
    },
    "./tabs": {
      "import": {
        "types": "./dist/tabs.d.ts",
        "default": "./dist/tabs.js"
      },
      "require": {
        "types": "./dist/tabs.d.cts",
        "default": "./dist/tabs.cjs"
      }
    },
    "./dialog": {
      "import": {
        "types": "./dist/dialog.d.ts",
        "default": "./dist/dialog.js"
      },
      "require": {
        "types": "./dist/dialog.d.cts",
        "default": "./dist/dialog.cjs"
      }
    },
    "./accordion": {
      "import": {
        "types": "./dist/accordion.d.ts",
        "default": "./dist/accordion.js"
      },
      "require": {
        "types": "./dist/accordion.d.cts",
        "default": "./dist/accordion.cjs"
      }
    },
    "./popover": {
      "import": {
        "types": "./dist/popover.d.ts",
        "default": "./dist/popover.js"
      },
      "require": {
        "types": "./dist/popover.d.cts",
        "default": "./dist/popover.cjs"
      }
    },
    "./tooltip": {
      "import": {
        "types": "./dist/tooltip.d.ts",
        "default": "./dist/tooltip.js"
      },
      "require": {
        "types": "./dist/tooltip.d.cts",
        "default": "./dist/tooltip.cjs"
      }
    },
    "./disclosure": {
      "import": {
        "types": "./dist/disclosure.d.ts",
        "default": "./dist/disclosure.js"
      },
      "require": {
        "types": "./dist/disclosure.d.cts",
        "default": "./dist/disclosure.cjs"
      }
    },
    "./navigation-menu": {
      "import": {
        "types": "./dist/navigation-menu.d.ts",
        "default": "./dist/navigation-menu.js"
      },
      "require": {
        "types": "./dist/navigation-menu.d.cts",
        "default": "./dist/navigation-menu.cjs"
      }
    },
    "./dropdown-menu": {
      "import": {
        "types": "./dist/dropdown-menu.d.ts",
        "default": "./dist/dropdown-menu.js"
      },
      "require": {
        "types": "./dist/dropdown-menu.d.cts",
        "default": "./dist/dropdown-menu.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "devDependencies": {
    "@data-slot/core": "workspace:*",
    "@data-slot/tabs": "workspace:*",
    "@data-slot/dialog": "workspace:*",
    "@data-slot/accordion": "workspace:*",
    "@data-slot/popover": "workspace:*",
    "@data-slot/tooltip": "workspace:*",
    "@data-slot/disclosure": "workspace:*",
    "@data-slot/navigation-menu": "workspace:*",
    "@data-slot/dropdown-menu": "workspace:*"
  },
  "keywords": [
    "headless",
    "ui",
    "components",
    "vanilla",
    "data-slot"
  ],
  "license": "MIT",
  "homepage": "https://github.com/bejamas/data-slot",
  "repository": {
    "type": "git",
    "url": "https://github.com/bejamas/data-slot",
    "directory": "packages/data-slot"
  }
}
